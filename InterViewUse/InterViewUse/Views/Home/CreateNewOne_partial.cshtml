@model InterViewUse.Models.Shippers
@using (Html.BeginForm("CreateNewOne_partial", "Home", FormMethod.Post))
{
    <h5  id="createnewoneTitle">新增一間運輸公司</h5>
    <div class="form-group">
        @Html.LabelFor(s => s.CompanyName):
        @Html.TextBoxFor(s => s.CompanyName, new { @class = "form-control", required = "required" })
    </div>
    <div class="form-group">
        @Html.LabelFor(s => s.Phone):
        @Html.TextBoxFor(s => s.Phone, new { @class = "form-control", required = "required" })
    </div>
    <div class="display_flex_justify-content_space-around">
        <button type="submit" class="btn btn-primary">新增</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">取消</button>
    </div>


}

